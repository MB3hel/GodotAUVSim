; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="Godot AUV Sim"
config/description="AUV simulator for AUVControlBoard math development"
run/main_scene="res://pool.tscn"
boot_splash/show_image=false
config/icon="res://images/icon.png"
config/macos_native_icon="res://images/icon.icns"
config/windows_native_icon="res://images/icon.ico"

[autoload]

Angles="*res://angles.gd"

[display]

window/size/width=1280
window/size/height=720
window/dpi/allow_hidpi=true

[global]

environment=false
resolution=false

[gui]

common/drop_mouse_on_gui_input_disabled=true

[physics]

common/enable_pause_aware_picking=true
3d/default_linear_damp=0.5
3d/default_angular_damp=0.5

[rendering]

quality/driver/fallback_to_gles2=true
environment/default_environment="res://default_env.tres"
